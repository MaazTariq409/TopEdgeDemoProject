@model IEnumerable<ScrapData>


<div class="d-flex justify-content-between align-items-center">
    <h1 class="text-primary">Scraped Data</h1>
    <div>
        <a class="btn btn-primary" asp-area="" asp-controller="Home" asp-action="uploadURl">Scrap Data</a>
    </div>
</div>

<div>
    <hr />
    <div class="container">
        @foreach (var item in Model)
        {
            <div class="row">
                <div class="col-sm-2 pb-2">
                    <img src="@Html.Raw(item.ImageUrl)" class="img-fluid">
                </div>
                <div class="col-sm-7 pb-2">
                    <h3>@Html.DisplayName(item.Name)</h3>
                </div>
                <div class="col-sm-3 navbtn">
                    <a href="@item.baseUrl" class="btn btn-primary">View Profile</a>
                    <a class="btn btn-danger" asp-controller="Home" asp-action="DeleteEntry" asp-route-Id="@item.Id">Delete Profile</a>
                </div>
            </div>
        }
    </div>
</div>
