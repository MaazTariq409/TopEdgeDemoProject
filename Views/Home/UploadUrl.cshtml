@model ScrapImageUploadDto;

<div class="row pt-2">
    <h1 class="text-primary">Upload Url</h1>
    <hr />
</div>

<form method="post" asp-action="uploadURl">
    <div class=" p-3">
        <div asp-validation-summary="All"></div>
        <div class="row">
            <div class="mb-3">
                <label asp-for="BaseUrl"></label>
                <input asp-for="BaseUrl" autofocus="true" class="form-control" />
                <span asp-validation-for="BaseUrl" class="text-danger"></span>
            </div>
            <div>
                <button class="btn btn-primary btn-lenght" type="submit">Submit</button>
            </div>
        </div>
    </div>
</form>

